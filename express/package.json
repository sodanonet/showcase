{
  "name": "express",
  "version": "1.0.0",
  "description": "DevBoard backend API with Express, MongoDB, JWT, Swagger, and TypeScript",
  "main": "src/server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
